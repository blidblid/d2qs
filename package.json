{
  "name": "d2queue",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-functions": "cd functions && tsc && cd ..",
    "build-all": "npm run build-functions && npm run build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "deploy-prod": "npm run build-all && firebase use default && firebase deploy",
    "deploy-dev": "npm run build-all && firebase use dev && firebase --config=./firebase-dev.json deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.12",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/fire": "7.1.1",
    "@angular/forms": "~12.2.0",
    "@angular/material": "12.2.12",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@berglund/firebase": "^0.0.5",
    "@berglund/material": "^0.0.4",
    "@berglund/mixins": "^0.0.2",
    "@berglund/rx": "^0.0.2",
    "firebase": "9.2.0",
    "rxfire": "^6.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1202.12",
    "@angular-devkit/build-angular": "~12.2.12",
    "@angular-devkit/schematics": "^12.2.12",
    "@angular/cli": "~12.2.12",
    "@angular/compiler-cli": "~12.2.0",
    "@schematics/angular": "^12.2.12",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "puppeteer": "^12.0.1",
    "typescript": "~4.3.5"
  }
}
